{
  "name": "goodtables-ui",
  "version": "1.0.0",
  "description": "Frontend for goodtables.io, built with Nuxt.js",
  "author": "Sam Smith",
  "private": true,
  "dependencies": {
    "nuxt": "v0.10.7"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "font": "cp -r node_modules/raleway-webfont/fonts static/fonts/raleway"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "bootstrap-sass": "^3.3.7",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "jquery": "^3.1.1",
    "node-sass": "^4.5.0",
    "parts-selector": "github:smth/parts-selector",
    "push-dir": "^0.4.1",
    "raleway-webfont": "^3.0.1",
    "sass-loader": "^6.0.2",
    "vuebar": "0.0.17"
  }
